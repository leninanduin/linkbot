{
    "manifest_version": 2,

    "name": "Tweetbotium",
    "description": "Replace twitter links for tweetbot calls.",
    "version": "1.0",

    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["jquery-1.11.1.min.js","urlreplace.js"],
            "all_frames":true,
            "css": ["tweetbotium.css"]
        }
    ],
    "web_accessible_resources":[
        "tweetbotium.css"
    ],
    "permissions": [
        "tabs",
        "https://*.twitter.com/",
        "http://*.twitter.com/",
        "https://twitter.com/"
    ],
    "icons":{
        "36": "icon-32.png",
        "48": "icon-48.png"
    }
}
